<%= semantic_form_for @rating_comment do |f| %>
  <%= f.inputs do %>
    <%= f.input :user_id %>
    <%= f.input :seller_id %>
    <%= f.input :item_id %>
    <%= f.input :rating %>
    <%= f.input :comment %>
    <%= f.input :is_seller_rating %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
